QSlider {
    background-color: transparent;
}

/* Groove (track) */
QSlider::groove:horizontal, QSlider::groove:vertical {
    background-color: rgba(120, 120, 120, 0.8); /* Subtle dark background */
    border-radius: 2px;
    border: none;
}

QSlider::groove:horizontal {
    height: 4px; /* Thinner groove */
}

QSlider::groove:vertical {
    width: 4px; /* Thinner groove */
}

/* Handle (thumb) */
QSlider::handle:horizontal, QSlider::handle:vertical {
    background-color: #B7B7B7; /* Light grey handle */
    border-radius: 6px; /* Slightly rounded corners */
    border: none;
}

QSlider::handle:horizontal {
    height: 12px; /* Smaller height */
    width: 12px;  /* Smaller width */
    margin: -4px 0; /* Adjusted margin for centering */
}

QSlider::handle:vertical {
    height: 12px; /* Smaller height */
    width: 12px;  /* Smaller width */
    margin: 0 -4px; /* Adjusted margin for centering */
}

/* Progress indicator (sub-page for horizontal, add-page for vertical) */
QSlider::sub-page:horizontal, QSlider::add-page:vertical {
    background-color: #479ef5; /* Blue accent for the progress part */
    border-radius: 2px;
}

/* Remainder of the track */
QSlider::add-page:horizontal, QSlider::sub-page:vertical {
    background-color: rgba(10, 10, 10, 0.8); /* Matches the groove background */
    border-radius: 2px;
}

